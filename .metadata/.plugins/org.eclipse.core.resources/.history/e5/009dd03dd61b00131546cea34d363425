	<!--페이지 크기 설정-->
	<style>
	
		
		#page{
			
			-webkit-transform-origin: top left;
			-moz-transform-origin: top left;
			-o-transform-origin: top left;
			-ms-transform-origin: top left;
		
			/*transform:scale(0.25);
			-webkit-transform: scale(0.25);
			-moz-transform: scale(0.25);
			-o-transform: scale(0.25);
			-ms-transform: scale(0.25);
			*/
			
			position: absolute;
			z-index: 1;
		}
		
		
		/*선택 표시*/
		#selectable .ui-selecting { background: #FECA40; }
		#selectable .ui-selected { background: #F39814; color: white; }
		
		
	</style>

	<script src="http://code.jquery.com/jquery-1.9.1.js"></script>
	<script src="http://code.jquery.com/ui/1.10.3/jquery-ui.js"></script>
	<!--<script src="jquery.transform2d.js"></script>-->
	
	<!--스크립트 추가-->
	<script>
			$(function(){
				
				// scale 설정
				var scaleFactor = 0.25;
				// 좌 상단 기준으로 사이즈 조절 될려면 transform-origin stayle 설정해 주어야 한다.
				$("#page").css("transform", "scale("+scaleFactor+")");
				
				/*
				// jquery.transform2d.js" : http://louisremi.github.io/jquery.transform.js/index.html
				$("#page").animate({
					transform: 'scale(' + scaleFactor + ')'
				});
				//
				*/
				
				// 모양 확인 위해 border 설정
				$("#page").css("border", "1px solid #FF0000");
				$(".t").each(
					function (e, ui){
						var element = $(ui);
						element.css("border", "1px solid #0000FF");
						// 드래그 기능
						drag(element);
					}
				);
				
				function drag(element)
				{
					//console.log($(element).attr("id"));
						
					// 스케일 적용된 drag 기능 참고
					// http://stackoverflow.com/questions/1061099/how-to-let-resize-vertically-a-div-with-only-jquery-no-plugins
					// http://gungfoo.wordpress.com/2013/02/15/jquery-ui-resizabledraggable-with-transform-scale-set/
					
					element.draggable({
						//handle: '.drag-handle',
						cursor:"move",
						containment:"parent",
						scroll:false,
						
						start: function(event, ui)
						{
							ui.position.left = 0;
							ui.position.top = 0;
						},
						drag: function(event, ui)
						{
							// TODO : Limit 체크 로직 추가
							
							// find change in left
							var changeLeft = ui.position.left - ui.originalPosition.left;
							// adjust new left by our zoomScale
							var newLeft = ui.originalPosition.left + changeLeft / scaleFactor;
							
							// find change in top
							var changeTop = ui.position.top - ui.originalPosition.top;
							// adjust new top by our zoomScale
							var newTop = ui.originalPosition.top + changeTop / scaleFactor;
				
							ui.position.left = newLeft;
							ui.position.top = newTop;
						}
					});
				}
			});
	</script>

